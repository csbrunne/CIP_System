<?xml version="1.0" encoding="utf-8"?>
<Routine Name="CS03_AutoLevel" Type="RLL">
  <RLLContent>
    <Rung Type="N">
      <CustomProperties>
        <Provider ID="ACM" Ext="Origin_0">
          <OriginHeader xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="1" Min="0" OExt="-1" UExt="1" />
        </Provider>
        <Provider ID="ACM" Ext="Origin_1">
          <Data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="3" Min="0" OExt="-1">
            <Val><![CDATA[<Origin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Owners>
    <OwnedBy InstName="EM_ReturnHeader3" LGuid="EE90A165-390A-4B4D-BCCE-F6CB44FC336C" />
  </Owners>
</Origin>]]></Val>
          </Data>
        </Provider>
      </CustomProperties>
      <Comment><![CDATA[===========================================================================
State 3 Command Sequence - Control Tank Level
===========================================================================
Open the Fresh Water Make-Up Valve if Level is Below Low Threshold.
Close the Fresh Water Make-Up Valve if Level is Above High Threshold.

Note - Step 999 is defined as Last Step, providing for one final scan
       of this routine before setting Cmd_StateComplete.]]></Comment>
      <Text><![CDATA[EQ(EM.Sts_eStep,999)OTL(EM.Cmd_StateComplete);]]></Text>
    </Rung>
    <Rung Type="N">
      <CustomProperties>
        <Provider ID="ACM" Ext="Origin_0">
          <OriginHeader xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="1" Min="0" OExt="-1" UExt="1" />
        </Provider>
        <Provider ID="ACM" Ext="Origin_1">
          <Data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="3" Min="0" OExt="-1">
            <Val><![CDATA[<Origin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Owners>
    <OwnedBy InstName="EM_ReturnHeader3" LGuid="EE90A165-390A-4B4D-BCCE-F6CB44FC336C" />
  </Owners>
</Origin>]]></Val>
          </Data>
        </Provider>
      </CustomProperties>
      <Text><![CDATA[EQ(EM.Sts_eStep,31)[OTL(wrkAcquire) ,XIC(wrkOwned) ADD(1,EM.Sts_eStep,EM.Sts_eStep) ];]]></Text>
    </Rung>
    <Rung Type="N">
      <CustomProperties>
        <Provider ID="ACM" Ext="Origin_0">
          <OriginHeader xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="1" Min="0" OExt="-1" UExt="1" />
        </Provider>
        <Provider ID="ACM" Ext="Origin_1">
          <Data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="3" Min="0" OExt="-1">
            <Val><![CDATA[<Origin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Owners>
    <OwnedBy InstName="EM_ReturnHeader3" LGuid="EE90A165-390A-4B4D-BCCE-F6CB44FC336C" />
  </Owners>
</Origin>]]></Val>
          </Data>
        </Provider>
      </CustomProperties>
      <Text><![CDATA[EQ(EM.Sts_eStep,32)[XIC(FillValve.Device1State) MOVE(34,EM.Sts_eStep) ,XIC(FillValve.Device0State) ADD(1,EM.Sts_eStep,EM.Sts_eStep) ];]]></Text>
    </Rung>
    <Rung Type="N">
      <CustomProperties>
        <Provider ID="ACM" Ext="Origin_0">
          <OriginHeader xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="1" Min="0" OExt="-1" UExt="1" />
        </Provider>
        <Provider ID="ACM" Ext="Origin_1">
          <Data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="3" Min="0" OExt="-1">
            <Val><![CDATA[<Origin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Owners>
    <OwnedBy InstName="EM_ReturnHeader3" LGuid="EE90A165-390A-4B4D-BCCE-F6CB44FC336C" />
  </Owners>
</Origin>]]></Val>
          </Data>
        </Provider>
      </CustomProperties>
      <Text><![CDATA[EQ(EM.Sts_eStep,33)[XIC(AI_Level.Sts_LowLimit) OTL(FillValve.Prog1Command) ,XIC(FillValve.Device1State) ADD(1,EM.Sts_eStep,EM.Sts_eStep) ];]]></Text>
    </Rung>
    <Rung Type="N">
      <CustomProperties>
        <Provider ID="ACM" Ext="Origin_0">
          <OriginHeader xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="1" Min="0" OExt="-1" UExt="1" />
        </Provider>
        <Provider ID="ACM" Ext="Origin_1">
          <Data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="3" Min="0" OExt="-1">
            <Val><![CDATA[<Origin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Owners>
    <OwnedBy InstName="EM_ReturnHeader3" LGuid="EE90A165-390A-4B4D-BCCE-F6CB44FC336C" />
  </Owners>
</Origin>]]></Val>
          </Data>
        </Provider>
      </CustomProperties>
      <Text><![CDATA[EQ(EM.Sts_eStep,34)[XIC(AI_Level.Sts_HighLimit) OTL(FillValve.Prog0Command) ,XIC(FillValve.Device0State) MOVE(33,EM.Sts_eStep) ];]]></Text>
    </Rung>
  </RLLContent>
</Routine>