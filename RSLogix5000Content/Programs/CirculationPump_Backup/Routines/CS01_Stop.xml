<?xml version="1.0" encoding="utf-8"?>
<Routine Name="CS01_Stop" Type="RLL">
  <RLLContent>
    <Rung Type="N">
      <CustomProperties>
        <Provider ID="ACM" Ext="Origin_0">
          <OriginHeader xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="1" Min="0" OExt="-1" UExt="1" />
        </Provider>
        <Provider ID="ACM" Ext="Origin_1">
          <Data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="3" Min="0" OExt="-1">
            <Val><![CDATA[<Origin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Owners>
    <OwnedBy InstName="EM_ReturnHeader3" LGuid="EE90A165-390A-4B4D-BCCE-F6CB44FC336C" />
  </Owners>
</Origin>]]></Val>
          </Data>
        </Provider>
      </CustomProperties>
      <Comment><![CDATA[===========================================================================
State 1 Command Sequence - Stop Pump (and Close All The Valves)
===========================================================================
Stop the pump, close all valves and release ownership to the Operator.

Note - Step 999 is defined as Last Step, providing for one final scan
       of this routine before setting Cmd_StateComplete.]]></Comment>
      <Text><![CDATA[EQ(EM.Sts_eStep,999)OTL(EM.Cmd_StateComplete);]]></Text>
    </Rung>
    <Rung Type="N">
      <CustomProperties>
        <Provider ID="ACM" Ext="Origin_0">
          <OriginHeader xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="1" Min="0" OExt="-1" UExt="1" />
        </Provider>
        <Provider ID="ACM" Ext="Origin_1">
          <Data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="3" Min="0" OExt="-1">
            <Val><![CDATA[<Origin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Owners>
    <OwnedBy InstName="EM_ReturnHeader3" LGuid="EE90A165-390A-4B4D-BCCE-F6CB44FC336C" />
  </Owners>
</Origin>]]></Val>
          </Data>
        </Provider>
      </CustomProperties>
      <Text><![CDATA[EQ(EM.Sts_eStep,11)[OTL(wrkAcquire) ,XIC(wrkOwned) ADD(1,EM.Sts_eStep,EM.Sts_eStep) ];]]></Text>
    </Rung>
    <Rung Type="N">
      <CustomProperties>
        <Provider ID="ACM" Ext="Origin_0">
          <OriginHeader xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="1" Min="0" OExt="-1" UExt="1" />
        </Provider>
        <Provider ID="ACM" Ext="Origin_1">
          <Data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="3" Min="0" OExt="-1">
            <Val><![CDATA[<Origin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Owners>
    <OwnedBy InstName="EM_ReturnHeader3" LGuid="EE90A165-390A-4B4D-BCCE-F6CB44FC336C" />
  </Owners>
</Origin>]]></Val>
          </Data>
        </Provider>
      </CustomProperties>
      <Text><![CDATA[EQ(EM.Sts_eStep,12)[OTL(PumpMotor.Prog0Command) ,XIC(PumpMotor.Device0State) ADD(1,EM.Sts_eStep,EM.Sts_eStep) ];]]></Text>
    </Rung>
    <Rung Type="N">
      <CustomProperties>
        <Provider ID="ACM" Ext="Origin_0">
          <OriginHeader xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="1" Min="0" OExt="-1" UExt="1" />
        </Provider>
        <Provider ID="ACM" Ext="Origin_1">
          <Data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="3" Min="0" OExt="-1">
            <Val><![CDATA[<Origin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Owners>
    <OwnedBy InstName="EM_ReturnHeader3" LGuid="EE90A165-390A-4B4D-BCCE-F6CB44FC336C" />
  </Owners>
</Origin>]]></Val>
          </Data>
        </Provider>
      </CustomProperties>
      <Text><![CDATA[EQ(EM.Sts_eStep,13)[OTL(SupplyValve.Prog0Command) OTL(RecircValve.Prog0Command) OTL(ReturnValve.Prog0Command) ,XIC(SupplyValve.Device0State) XIC(RecircValve.Device0State) XIC(ReturnValve.Device0State) ADD(1,EM.Sts_eStep,EM.Sts_eStep) ];]]></Text>
    </Rung>
    <Rung Type="N">
      <CustomProperties>
        <Provider ID="ACM" Ext="Origin_0">
          <OriginHeader xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="1" Min="0" OExt="-1" UExt="1" />
        </Provider>
        <Provider ID="ACM" Ext="Origin_1">
          <Data xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" Maj="3" Min="0" OExt="-1">
            <Val><![CDATA[<Origin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Owners>
    <OwnedBy InstName="EM_ReturnHeader3" LGuid="EE90A165-390A-4B4D-BCCE-F6CB44FC336C" />
  </Owners>
</Origin>]]></Val>
          </Data>
        </Provider>
      </CustomProperties>
      <Text><![CDATA[EQ(EM.Sts_eStep,14)OTU(wrkAcquire)XIO(EM.Sts_Ovrd)MOVE(999,EM.Sts_eStep);]]></Text>
    </Rung>
  </RLLContent>
</Routine>